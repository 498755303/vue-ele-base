{
  "name": "vue-ele-ts-base",
  "version": "1.0.1",
  "private": false,
  "author": "hl <498755303@qq.com.com>",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service lint && vue-cli-service build",
    "lint": "vue-cli-service lint --fix",
    "build:dll": "vue-cli-service build --report-json --report",
    "build:deploy": "vue-cli-service lint && vue-cli-service build --mode deploy --dest dist-deploy",
    "build:test": "vue-cli-service lint && vue-cli-service build --mode test --dest dist-test",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -w -r 0",
    "dll": "webpack -p --progress --config ./webpack.dll.conf.js",
    "sentry-clear": "sentry-cli releases -o orgName -p projectName files staging@online delete --all",
    "sentry-clear:deploy": "sentry-cli releases -o orgName -p projectName files staging@deploy delete --all",
    "serve:online": "vue-cli-service serve --mode production",
    "serve:deploy": "vue-cli-service serve --mode deploy",
    "styleguide": "vue-cli-service styleguidist",
    "styleguide:build": "vue-cli-service styleguidist:build --unhandled-rejections=strict",
    "test:cov": "vue-cli-service test:unit --coverage",
    "test:unit": "vue-cli-service test:unit"
  },
  "main": "index.js",
  "dependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@riophae/vue-treeselect": "^0.4.0",
